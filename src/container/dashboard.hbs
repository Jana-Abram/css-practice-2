      </div>
      <div class="purchase--item">
        <div class="purchase--info text">Customer</div>
        <div class="purchase--text text">dima@com.ua</div>
      </div>
    </div>
  </div>
  
  <div class="payment">
      <div class="payment__header text">Payment</div>
    <div class="divider"></div>
    <div class="payment--container">
      <div class="checkbox--item">
        <input type="radio" class="radio--input"  id="Credit card">
        <label for="checkbox" class="payment--text text">Credit card</label>
      </div>
      <div class="checkbox--item">
        <input type="radio" class="radio--input" id="Debit card">
        <label for="checkbox" class="payment--text text">Debit card</label>
      </div>
      <div class="checkbox--item">
        <input type="radio" class="radio--input"  id="Paypal">
        <label for="checkbox" class="payment--text text">PayPal</label>
      </div>
    </div>
    
 <div class="input--container">
      <div class="input--item">
        <label for="input" class="label text">Name on card</label>
        <input type="text" id="input">
      </div>
      <div class="input--item">
        <label for="input" class="label text">Credit card number</label>
        <input type="text" id="input">
      </div>
      <div class="input--item">
        <label for="input" class="label text">Expiration</label>
        <input type="text" id="input">
      </div>
      <div class="input--item">
        <label for="input" class="label text">CVV</label>
        <input type="text" id="input">
      </div>
  </div>
<div class="divider"></div>

      <div class="select-container">
        <div class="select-item">
          <label for="select">Country</label>
          <select id="select">
            <option value="option">Choose...</option>
            <option value="option">Option 2</option>
            <option value="option">Option 3</option>
          </select>
        </div>
        <div class="select-item">
          <label for="select">State</label>
          <select id="select">
            <option value="option">Choose...</option>
            <option value="option">Option 2</option>
            <option value="option">Option 3</option>
          </select>
          
        </div>
         </div>
      <div class="divider"></div>
      <button class="primary--button">Continue to checkout</button>
    </div>
      </div>
    </div>
  </div>
</div>
        </main>
       </div>

    </body>
		
</section>